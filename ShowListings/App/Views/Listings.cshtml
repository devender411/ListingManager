<div class="container">
    <div class="page-header">
        <h1>Listings</h1>
    </div>


    <div class="table-wrap">
        <table class='table table-striped table-bordered table-hover' id='clientTable'>
            <colgroup>
                <col style="width: 1px" />
                <col style="width: 100px" />
                <col style="width: 100px" />
                <col style="width: 100px" />
                <col style="width: 200px" />
            </colgroup>
            <thead class="thead-inverse">
                <tr>
                    <th class="text-xs-center">Id</th>
                    <th>Code</th>
                    <th class="text-xs-center">Name</th>
                    <th class="text-xs-center">Address</th>
                    <th class="text-xs-center"></th>
                </tr>
            </thead>
            <tbody ng-repeat='listing in Listings'>
                <tr>
                    <td class="text-xs-center">{{ listing.Listing_Id }}</td>
                    <td class="text-xs-center">{{ listing.ListingCode }}</td>
                    <td class="text-xs-center">{{ listing.ListingName }}</td>
                    <td class="text-xs-center">{{ listing.ListingAddress }}</td>
                    <td class="text-xs-center">
                        <a class='btn btn-primary' href='' ng-click='edit(listing.Listing_Id)'>Edit</a>
                        <a class='btn btn-danger' href='' ng-click="delete(listing.Listing_Id)">Delete</a>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>